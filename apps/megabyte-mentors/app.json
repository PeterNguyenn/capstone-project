{
  "expo": {
    "name": "MegabyteMentors",
    "slug": "megabyte-mentors",
    "scheme": "megabyte-mentors",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "notification": {
      "icon": "./assets/images/logo-small.png",
      "color": "#ffffff",
      "androidMode": "default",
      "androidCollapsedTitle": "New notification"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thongnguyensheridan.megabytementors",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "notifications": {
        "icon": "./assets/images/logo-small.png",
        "color": "#ffffff"
      },
      "package": "com.thongnguyensheridan.megabytementors"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "66727027-3a94-4329-bb13-d6f792746f29"
      }
    }
  }
}
